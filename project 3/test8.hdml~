def fun1(a) = fun1

def fun2(_) = if fun2
    then fun2
    else ~fun2

def fun3(a,b,c) = let a = b[c] in [c]

def fun4(q,w,e,r,t,y,u,i,o,p,a,s,d,f,g,h,j,k,l,z,x,c,v,b,n,m) = q+w+e+r+t+y+u+i+o+p+a+s+d+f+g+h+j+k+l+z+x+c+v+b+n+m

def fun5(_,_,_) = #fun5

def fun6(a,b,c,d) = (a+b)*(c+d)

def fun7(a) if a[a..a] 
    then a[a]
    else a

def fun8(qwerty) = qwerty = 12345

def fun9(abc) = let abc = cba in reverse (*poprawny komentarz (*i def ktore sie nie wywola*)*)

def fun10('abC_d) = if 
    let 'abc_d = 'abC_d in fun9
    then
    let 'abc_d = 'abC_d in fun9
    else
    let 'abC_d = [] in fun9('abc_d)
